{
  "name": "jeyroik/extas-repositories-mongo",
  "description": "Extas compatable Mongo repository package",
  "require": {
    "alcaeus/mongo-php-adapter": "1.*",
    "league/monga": "1.*",
    "jeyroik/extas-foundation": "6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "vlucas/phpdotenv": "^3.4",
    "jeyroik/extas-snuffbox": "1.*"
  },
  "autoload": {
    "psr-4": {
      "extas\\": "src/",
      "tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox --coverage-clover=coverage.xml --whitelist=src tests"
  }
}